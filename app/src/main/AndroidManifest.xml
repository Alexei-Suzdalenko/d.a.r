<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    package="drugaya.astrajan.radio">

    <uses-permission android:name="android.permission.INTERNET" />
    <uses-permission android:name="android.permission.ACCESS_NETWORK_STATE" />
    <uses-permission android:name="android.permission.FOREGROUND_SERVICE" />
    <uses-permission android:name="android.permission.WAKE_LOCK" />
    <uses-permission android:name="android.permission.RECEIVE_BOOT_COMPLETED" />
    <application
        android:name=".rossiya_app.util.App"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/Theme.Drugayaastrajanradio"
        android:usesCleartextTraffic="true"
        tools:targetApi="m"
        android:allowBackup="false">
        <activity
            android:name=".MainActivity"
            android:exported="true"
            android:label="@string/title_activity_index_pages" >
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />
                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>

        <service android:name=".rossiya_app.util.ServiceRadio" android:foregroundServiceType="location" />
        <meta-data android:name="com.google.android.gms.ads.APPLICATION_ID" android:value="ca-app-pub-7286158310312043~4951140513" />

        <receiver android:name=".assets.OffRadioReceiver" android:process=":remote" />
        <receiver android:name=".assets.AutoStart"
            android:exported="true">
            <intent-filter>
                <action android:name="android.intent.action.BOOT_COMPLETED" />
            </intent-filter>
        </receiver>
        <service
            android:name="drugaya.astrajan.radio.components.files.dont_use_file.YourService"
            android:enabled="true"
            android:process=":your_service" />
    </application>

</manifest>